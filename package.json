{
  "name": "prompt-grader",
  "version": "1.0.0",
  "description": "A tool to help users create better prompts with real-time grading and suggestions",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/index.js",
    "client": "cd src/client && npm start",
    "build": "cd src/client && npm run build",
    "install-all": "npm install && cd src/client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["prompt", "grading", "ai", "react", "node"],
  "author": "",
  "license": "MIT"
}